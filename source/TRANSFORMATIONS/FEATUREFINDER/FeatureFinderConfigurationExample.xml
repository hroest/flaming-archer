<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- Sample FeatureFinder configuration-->
<!-- For the meaning of the individual parameters
please have a look at the corresponding header files. -->
<PARAMETERS>
 
  <NODE name="Seeders">
    <NODE name="SimpleSeeder">
      <ITEM name="ID" value="SimpleSeeder" type="string"/>
      <ITEM name="intensity_perc" value="0.3" type="float"/>
    </NODE>
  </NODE>

  <NODE name="Extenders">
    <NODE name="SimpleExtender" >
      <ITEM name="ID" value="SimpleExtender" type="string" />
      <ITEM name="dist_mz_up" value="4" type="float" />
      <ITEM name="dist_mz_down" value="4" type="float" />
      <ITEM name="dist_rt_up" value="5.0" type="float" />
      <ITEM name="dist_rt_down" value="5.0" type="float" />
      <ITEM name="priority_thr" value="0.001" type="float"/>
     </NODE>
  </NODE>

  <NODE name="ModelFitters">
    <NODE name="SimpleModelFitter" >
      <ITEM name="ID" value="SimpleModelFitter" type="string" />
			<ITEM name="interpolation_step_mz" value="0.03" type="float" />
			<ITEM name="interpolation_step_rt" value="0.2" type="float" />
			<ITEM name="tolerance_stdev_bounding_box" value="3.0" type="float" />
			<ITEM name="stdev_first_mz" value="0.01" type="float"/>
			<ITEM name="stdev_last_mz" value="0.2" type="float"/>
			<ITEM name="stdev_step_mz" value="0.01" type="float"/>
			<ITEM name="quality" value="Correlation" type="string" />
			<ITEM name="min_quality" value="0.4" type="float" />
			<ITEM name="min_set_size" value="5" type="int" />
			<ITEM name="intensity_cutoff_factor" value="0.1" type="float"/>
		</NODE>
	</NODE>

</PARAMETERS>
